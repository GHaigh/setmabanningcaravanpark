{
  "version": 2,
  "name": "setmabanning-caravan-park",
  "buildCommand": "echo 'Static site - no build required'",
  "outputDirectory": ".",
  "installCommand": "echo 'No dependencies to install'",
  "framework": null,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "GOOGLE_MAPS_API_KEY": "@google_maps_api_key"
  },
  "functions": {
    "index.html": {
      "runtime": "@vercel/static"
    }
  }
}
